{
  "name": "landing_web",
  "version": "1.0.0",
  "description": "web 2.0",
  "scripts": {
    "dev": "npm run build:css && cross-env NODE_ENV=development webpack-dev-server --config webpack.config.ts --env.env=dev --open --hot",
    "dev:start": "nodemon ./server-build/index.js",
    "build": "npm run build:css && webpack --config webpack.config.ts --env.env=prod && cross-env webpack --config webpack.server.ts --mode=production",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "build-storybook": "build-storybook",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,scss,css}\" --write",
    "lint": "eslint ./src --cache && echo \"No linting errors\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "pretty": "prettier --check 'src/**/*.js'",
    "pretty:w": "prettier --write 'src/**/*.[js|ts]x?",
    "production:start": "cross-env NODE_ENV=production nodemon ./server-build/index.js",
    "staging:start": "cross-env NODE_ENV=staging nodemon ./server-build/index.js",
    "start": "node ./server-build/index.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:w": "jest --watch",
    "webpack": "webpack -wd --env.env=dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calizahq/landing_web.git"
  },
  "proxy": "http://localhost:3000",
